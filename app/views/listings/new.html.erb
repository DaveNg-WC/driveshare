<h1>New Listing</h1>
<%= form_for @listing do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :description %>
  <%= f.text_area :description %><br>

  <%= f.label :price %>
  <%= f.text_field :price %><br>

  <%= f.label :address %>
  <%= f.text_field :address %><br>

  <%= f.submit "Create Listing" %>
<% end %>
